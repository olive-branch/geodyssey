{
  "name": "geodyssey",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "vue-cli-service serve --port 4000",
    "test": "jest",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start:server": "cd src/api && npm start",
    "build:server": "cd src/api && npm run build",
    "install": "cd src/api && npm ci"
  },
  "dependencies": {
    "buefy": "^0.8.20",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.3.1",
    "vue-toast-notification": "^0.4.1",
    "vue2-datepicker": "^3.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli": "^4.4.1",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "^4.4.1",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.5",
    "vercel": "^19.0.1",
    "vue-template-compiler": "^2.6.11",
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-unused-vars": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">= 14.0.0"
  }
}
